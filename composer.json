{
  "name": "steveolotu/featuredocu",
  "description": "Uses annotations to auto-generate documentation of business logic features in the code.",
  "type": "symfony-bundle",
  "keywords": [
    "symfony", "documentation", "annotations"
  ],
  "require": {
    "doctrine/annotations": "^1.11",
    "doctrine/common": "^3.1",
    "php": ">=7.3",
    "symfony/framework-bundle": "^5.0",
    "symfony/http-client": "^5.0",
    "symfony/twig-bundle": "^5.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16.7",
    "phpstan/phpstan": "^0.12.56",
    "phpunit/phpunit": "^9.5.0",
    "matthiasnoback/symfony-dependency-injection-test": "^4.2",
    "nyholm/symfony-bundle-test": "^1.6"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Steve Olotu",
      "email": "mail@steveolotu.com"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "SteveOlotu\\FeatureDocu\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SteveOlotu\\FeatureDocu\\Tests\\": "tests/"
    }
  }
}
